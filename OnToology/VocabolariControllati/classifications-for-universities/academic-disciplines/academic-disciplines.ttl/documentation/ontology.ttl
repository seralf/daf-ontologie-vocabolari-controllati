                                                                                                                                                              <https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/academic-disciplines/SSD-FIS-08> ;
                                                                                                               <http://www.w3.org/2004/02/skos/core#notation> "02/D1" ;
                                                                                                               <http://www.w3.org/2004/02/skos/core#prefLabel> "Applied physics, physics teaching and history of physics"@en ,
                                                                                                                                                               "Fisica applicata, didattica e storia della fisica"@it ;
                                                                                                               <https://w3id.org/italia/onto/CLV/hasRankOrder> "3" .


