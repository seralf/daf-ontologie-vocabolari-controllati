                                                                                                                                                              <https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/academic-disciplines/SSD-BIO-15> ;
                                                                                                               <http://www.w3.org/2004/02/skos/core#notation> "05/A1" ;
                                                                                                               <http://www.w3.org/2004/02/skos/core#prefLabel> "Botanica"@it ,
                                                                                                                                                               "Botany"@en ;
