                                                             owl:onProperty <https://w3id.org/italia/onto/CLV/hasAdressinTime> ;
                                                             owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                             owl:onClass <https://w3id.org/italia/onto/CLV/AddressInTime>
                                                           ] ,
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty <https://w3id.org/italia/onto/CLV/hasCensusSection> ;
